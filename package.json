{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "dompurify": "^2.2.9",
    "eslint-plugin-compat": "^3.11.1",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.0",
    "pretty": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dj-table": "^1.1.8",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-select": "^4.3.1",
    "semantic-release": "^17.4.4",
    "use-debounce": "^7.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "examples/basic/*.json",
      "examples/full/*.json"
    ],
    "collectCoverageFrom": [
      "src/components/react-dj-table/*.js",
      "src/components/react-dj-table/children/*.js",
      "src/components/react-dj-table/reducers/*.js",
      "src/components/react-dj-table/utils/*.js"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "coverage": "craco test --coverage --watchAll=false --testResultsProcessor jest-sonar-reporter",
    "docs": "jsdoc -c jsdoc.conf.json",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.1.10",
    "all-contributors-cli": "^6.20.0",
    "autoprefixer": "^9.8.6",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest-sonar-reporter": "^2.0.0",
    "parcel": "^2.0.0-beta.3.1",
    "postcss": "^7.0.36",
    "prettier": "^2.3.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.2",
    "web-vitals": "^1.1.2"
  },
  "release": {
    "branches": [
      "main",
      "develop"
    ]
  }
}